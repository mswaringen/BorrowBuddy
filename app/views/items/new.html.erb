<div class="container">

<h1 align="center">List your item</h1>
<div class="form-inputs">

<%= simple_form_for @item do |f| %>
  <%= f.input :title %>
  <%= f.input :price %>
  <%= f.input :availability %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden  %>
  <%= f.submit class:"form-actions btn btn-outline-dark btn-sm" %>
<% end %>
</div>
</div>

